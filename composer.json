{
    "name": "setono/post-nord-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle that integrates the PostNord PHP SDK",
    "keywords": [
        "symfony",
        "setono",
        "postnord",
        "post nord"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "setono/post-nord-php-sdk": "^1.0",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.3",
        "korbeil/phpstan-generic-rules": "^0.2.4",
        "kriswallsmith/buzz": "^1.1",
        "matthiasnoback/symfony-config-test": "^4.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "nyholm/psr7": "^1.1",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.19",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "phpstan/phpstan-webmozart-assert": "^0.12.3",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "sylius-labs/coding-standard": "^3.1",
        "symplify/coding-standard": "^8.2",
        "thecodingmachine/phpstan-safe-rule": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\PostNordBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Setono\\PostNordBundle\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "./vendor/bin/phpstan analyse -c phpstan.neon -l max src",
        "check-style": "./vendor/bin/ecs check --ansi src/ tests/",
        "fix-style": "./vendor/bin/ecs check --fix --ansi src/ tests/",
        "test": "./vendor/bin/phpunit"
    }
}
